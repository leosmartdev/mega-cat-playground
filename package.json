{
  "name": "megacat-marketplace-server",
  "version": "1.0.0",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "description": "MegaCat Studios Whitelabel Marketplace Restful API with Node.js, Express and MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/src/server.js --environment production",
    "staging": "node ./dist/src/server.js --environment staging",
    "bkcn-staging": "node ./dist/src/server.js --environment bkcn-staging",
    "bkcn-production": "node ./dist/src/server.js --environment bkcn-production",
    "build": "rimraf ./dist && tsc && npm run copy-files",
    "copy-files": "npm run copy-static && npm run copy-keys && npm run copy-swagger",
    "copy-static": "cp -r ./src/static/ ./dist/src/static",
    "copy-keys": "cp -r ./keys/ ./dist/keys",
    "copy-swagger": "cp -r ./public ./dist/public",
    "swagger": "tsoa spec",
    "start-dev": "nodemon ./src/server.ts --environment dev",
    "bkcn-dev": "nodemon ./src/server.ts --environment bkcn-dev",
    "start:cron:mcl:dev": "ts-node ./src/startCronJobs.ts --environment dev",
    "start:cron:mcl:staging": "ts-node ./src/startCronJobs.ts --environment staging",
    "start:cron:mcl:production": "ts-node ./src/startCronJobs.ts --environment production",
    "start:cron:bkcn:dev": "ts-node ./src/startCronJobs.ts --environment bkcn-dev",
    "start:cron:bkcn:staging": "ts-node ./src/startCronJobs.ts --environment bkcn-staging",
    "start:cron:bkcn:production": "ts-node ./src/startCronJobs.ts --environment bkcn-production",
    "test": "jest --config=jest.config.js --silent",
    "lint": "npx prettier --check \"src/**/*.{ts,js,json}\"",
    "lint:fix": "npx prettier --write \"src/**/*.{ts,js,json}\""
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "nikko-megacat",
  "license": "ISC",
  "dependencies": {
    "@googleapis/sheets": "^0.3.0",
    "@mapbox/node-pre-gyp": "^1.0.8",
    "@socket.io/redis-adapter": "^7.1.0",
    "@socket.io/redis-emitter": "^4.1.1",
    "@types/node": "^17.0.10",
    "aws-sdk": "^2.973.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "chroma-js": "^2.1.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "googleapis": "^95.0.0",
    "jest": "27.4.7",
    "joi": "17.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "minimist": "^1.2.5",
    "mongoose": "5.13.14",
    "mongoose-sequence": "5.3.1",
    "node-cron": "3.0.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "redis": "4.0.4",
    "socket.io": "4.4.1",
    "supervisor": "^0.12.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.4.0",
    "tsoa": "^3.14.1",
    "tunnel-ssh": "^4.1.6",
    "typescript": "^4.5.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-fileupload": "1.2.2",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.6",
    "@types/minimist": "1.2.2",
    "@types/node-cron": "3.0.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/tunnel-ssh": "4.1.1",
    "jest-extended": "1.2.0",
    "mongodb-memory-server": "^8.2.0",
    "nodemon": "2.0.15",
    "socket.io-client": "4.4.1",
    "supertest": "^6.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0"
  }
}
